*
	margin 0
	padding 0
	outline none
body
	font-size: 16px
	font-family: G
	color #000
	background-color #fff
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	text-align: center
	margin 0 auto
	border-radius: 5px;
	width: 100%
	max-width: 250px;
	// height 64px
	// line-height: 64px
	padding 20px 10px 21px
	background-color: MB
	color #fff
	font-family: G
	transition: all .6s ease
	&:focus
		text-decoration: none
		color #fff
	&:hover
		text-decoration: none
		color #fff
		box-shadow: 0 -150px 0 0 MB + 15% inset
.general-title
	text-align: center
	font-size: 25px;
	font-family: GBd
	color: rgb(0, 0, 0);
	text-transform: uppercase;
	line-height: 1
	margin-bottom: 45px