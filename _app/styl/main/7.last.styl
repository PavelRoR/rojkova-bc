#last
	padding 50px 0
	background-color: #15182A
	background-image url(../img/backs/last-bg.png)
	background-repeat no-repeat
	background-position: center bottom
	background-size: cover
	text-align: center
	color #fff
	text-transform: uppercase
	font-family: GBk
.last-title
	font-size: 50px
.last-subtitle
	font-size: 30px
	margin 50px auto
	width 100%
	max-width 700px
.last-text
	font-size: 20px
	margin-bottom: 40px
.button-last
	background-color: transparent
	border: 2px solid #fff
	padding 18px 10px 19px
